		"auth:generate": "bunx @better-auth/cli@latest generate --output src/lib/db/schemas/_auth.ts --config src/lib/auth/config.ts --y",
		"build": "bun run build:worker && next build --turbopack",
		"build:worker": "bun run scripts/workers/index.ts",
		"check": "tsgo --noEmit && biome check --fix --diagnostic-level=error",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:publish": "drizzle-kit generate && bun run scripts/publishSchema.ts",
